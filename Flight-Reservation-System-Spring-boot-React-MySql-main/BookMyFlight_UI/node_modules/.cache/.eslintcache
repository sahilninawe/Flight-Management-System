h('/'),null\n    }\n\n    else{\n    return (\n        <div style={{paddingTop : 70}}>\n            <Header />\n        <div className=\"p-3\" style={styling.wrapper}>\n            \n            <div className=\"card m-auto\" style={{width: \"23rem\"}}>\n                <img className=\"card-img-top m-auto mt-3 w-50\" src={planeTicket} alt=\"admin\" />\n                <div className=\"card-body\">\n                    <h5 className=\"card-title\">Welcome </h5>\n                    <p className=\"card-text\">As Admin you can add schedules for flight, modify and delete the schedules </p>\n                </div>\n                <ul className=\"list-group list-group-flush\">\n                    <li className=\"list-group-item\">\n                        <Link className=\"card-link\" to=\"/addFlight\">Add Flights</Link>\n                    </li>\n                    <li className=\"list-group-item\">\n                        <Link className=\"card-link\" to=\"/allFlights\">All Flight</Link>\n                    </li>\n                    <li className=\"list-group-item\"></li>\n                </ul>\n            </div>\n            \n        </div>\n            <Footer/>\n        </div>\n    );\n    }\n};\n\nlet styling = {\n    wrapper : {\n        height : \"80vh\",\n        background: `url(${plane})`\n    }\n}\n\nexport default Admin;","C:\\Users\\2317565\\OneDrive - Cognizant\\Desktop\\FMS\\Flight-Reservation-System-Spring-boot-React-MySql-main\\BookMyFlight_UI\\src\\components\\FlightListAdmin.js",[],"C:\\Users\\2317565\\OneDrive - Cognizant\\Desktop\\FMS\\Flight-Reservation-System-Spring-boot-React-MySql-main\\BookMyFlight_UI\\src\\components\\ErrorWorld.js",[],"C:\\Users\\2317565\\OneDrive - Cognizant\\Desktop\\FMS\\Flight-Reservation-System-Spring-boot-React-MySql-main\\BookMyFlight_UI\\src\\components\\Ticket.js",[],"C:\\Users\\2317565\\OneDrive - Cognizant\\Desktop\\FMS\\Flight-Reservation-System-Spring-boot-React-MySql-main\\BookMyFlight_UI\\src\\componen